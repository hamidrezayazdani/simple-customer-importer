{
    "name": "hamidreza-yazdani/simple-customer-importer",
    "description": "A WordPress plugin to import customers from an Excel file.",
    "type": "wordpress-plugin",
    "require": {
        "php": "^7.4",
        "phpoffice/phpspreadsheet": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "SimpleCustomerImporter\\": "includes/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "platform-check": false
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
